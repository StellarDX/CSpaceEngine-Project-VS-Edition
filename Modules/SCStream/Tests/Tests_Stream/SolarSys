DwarfMoon	"Phobos/Mars I"
{
	ParentBody     "Mars"
	Class          "Asteroid"

	RadiusInfo      11.1
	Radius          8.102745 // bump map is zero-based
	Mass            1.79E-9

	AlbedoBond      0.02
	AlbedoGeom      0.07
	Brightness      0.4
	BrightnessReal  0.15

	// Precise data taken from:
	// ftp://ssd.jpl.nasa.gov/pub/eph/satellites/orientation/iom.392R-17-004.pdf
	RotationModel "IAU"
	{
		Epoch			2451545			// J2000
		PoleRA      	317.670722		// degrees
		PoleRARate  	-0.1084132486	// degrees/century
		PoleDec     	52.886261		// degrees
		PoleDecRate 	-0.0613383599	// degrees/century
		PrimeMeridian	35.187741		// degrees
		RotationRate	1128.8447592753	// degrees/day
		RotationAccel	12.722			// degrees/century^2
		PeriodicTermsSecular			// SUM { amp[i] * sin(phase[i] + frequency[i] * t_centuries + freq_rate[i] * t_centuries^2) }
		{
			// PoleRA amp, PoleDec amp, PrimeMer amp, phase, freq, freq_rate
			-1.784328, -1.075191,  1.424227, 190.752833, 15917.1197346, 0
			 0.022113,  0.006656, -0.022751,  21.510735, 31834.5296590, 0
		}
	}

	Interior
	{
		Composition // mass fraction, values in percent
		{
			Silicates	100
		}
	}

	Surface
	{
		// Surface map author: Phil Stooke
		DiffMap        "Phobos/Surface-PS"

		ModulateColor  (1.0 0.88 0.76)

		// Elevation map author: Vladimir Romanyuk "SpaceEngineer"
		BumpMap        "Phobos/Bump-SE"
		BumpHeight      5.8051
		BumpOffset      0.0

		Hapke           1.0
		SpotBright      5.7		// Simonelli et al., 1998
		SpotWidth       0.072	// Simonelli et al., 1998
	}

	Orbit
	{
		AnalyticModel  "Phobos-NOE-4"
		RefPlane       "Equator"
